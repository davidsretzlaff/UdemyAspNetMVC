@model IEnumerable<learnaspnet.Models.Filme>

@{
    ViewBag.Title = "GetFilmes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Filmes</h2>
 <table class="table table-bordered table-hover">    
    <thead>        
        <tr>            
            <th>Filme</th>            
        </tr>        
    </thead>    
     <tbody>
         @foreach (var filme in Model)
         {
           <tr>
             <td> @filme.Nome </td >
           </tr >
         }
     </tbody>    
</table>

